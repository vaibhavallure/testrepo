<config>
    <modules>
        <Mage_Shipping>
            <version>1.6.0.0</version>
        </Mage_Shipping>
    </modules>
    <default>
        <carriers>
             <bakerloo_store_pickup>
                <active>0</active>
                <price><![CDATA[0.00]]></price>
                <sort_order>20</sort_order>
                <sallowspecific>0</sallowspecific>
                <showmethod>0</showmethod>
                <!--<cutoff_cost>50</cutoff_cost>-->
                <model>bakerloo_shipping/carrier_storePickup</model>
                <name>Store Pickup</name>
                <title>Store Pickup</title>
                <render_as>select</render_as>
                <specificerrmsg>This shipping method is currently unavailable. If you would like to ship using this shipping method, please contact us.</specificerrmsg>
                <ship>0</ship>
            </bakerloo_store_pickup>
        </carriers>
    </default>
</config>
