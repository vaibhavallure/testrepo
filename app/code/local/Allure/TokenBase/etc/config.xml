<?xml version="1.0"?>
<config>
	<modules>
		<Allure_TokenBase>
			<name>Allure TokenBase</name>
			<version>0.1.0</version>
		</Allure_TokenBase>
	</modules>
	
	<global>
	
		<controllers>
             <Allure_TokenBase>
                  <rewrite>
                      <customer>Allure_TokenBase_Customer_Paymentinfo</customer>
                  </rewrite>
             </Allure_TokenBase>
        </controllers> 
        
        <resources>
            <allure_paradoxlabs_tokenbase_setup>
                <setup>
                    <module>Allure_TokenBase</module>
                </setup>
            </allure_paradoxlabs_tokenbase_setup>
        </resources> 
		
	</global>
	
	<frontend>
		<routers>
			<customer>
                <args>
                    <modules>
                       <Allure_TokenBase before="ParadoxLabs_TokenBase">Allure_TokenBase_Customer</Allure_TokenBase>
                    </modules>
                </args>
            </customer>
		</routers>
    </frontend>
	
</config>