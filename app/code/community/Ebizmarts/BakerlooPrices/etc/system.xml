<?xml version="1.0"?>
<config>
    <sections>
        <bakerloorestful>
            <groups>
                <checkout>
                    <fields>
                        <price_override translate="label comment">
                            <label>Match order prices to POS prices</label>
                            <frontend_type>select</frontend_type>
                            <source_model>adminhtml/system_config_source_yesno</source_model>
                            <comment>
                                <model>bakerloo_prices/adminhtml_comment</model>
                            </comment>
                            <sort_order>100</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>0</show_in_website>
                            <show_in_store>0</show_in_store>
                            <can_be_empty>0</can_be_empty>
                        </price_override>
                    </fields>
                </checkout>
            </groups>
        </bakerloorestful>
    </sections>
</config>